<h3> List of all Buildings/Locations</h3>

<% @buildings.sort_by{|b| b[:location_name]}.each do |building|%>
    <ul>
        <li> <%= link_to building.location_name, building_path(building)%> - <%= building.address%> </li>
    </ul>
<% end %>

<br/>
<p><%= link_to "Go Back", user_path(@user.id) %></p>